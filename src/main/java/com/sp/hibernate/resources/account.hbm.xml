<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
    <class name="com.sp.hibernate.Beans.Account" table="account_info">
        <id name="accountnumner" column="account_number">
        </id>
        <property name="first_name" column="first_name"/>
        <property name="last_name" column="last_name"/>
        <property name="account_balance" column="account_balance"/>
        <property name="city" column="city"/>
    </class>
</hibernate-mapping>
